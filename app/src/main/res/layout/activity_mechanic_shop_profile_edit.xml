<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".views.mechanic_shop_profile_edit">

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3">

        <ImageView
            android:id="@+id/BlurImageView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/bg1"
            android:scaleType="centerCrop"/>

        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center_horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/img_mechanic_se_shop_logo"
                    android:layout_width="110dp"
                    android:layout_height="110dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/blank_user"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="35dp"/>

                <TextView
                    android:id="@+id/tv_mechanic_se_shop_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="Owner Name"
                    android:textSize="25dp"
                    android:textStyle="bold"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/exo_semibold"
                    android:textColor="@color/cardview_light_background"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/img_mechanic_se_shop_logo"/>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>



    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="6"
        android:layout_gravity="center"
        android:paddingTop="15dp"
        android:paddingLeft="25dp"
        android:paddingRight="25dp"
        android:orientation="vertical">

        <TextView
            android:textStyle="bold"
            android:paddingLeft="20dp"
            android:id="@+id/pto"
            android:textSize="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:fontFamily="@font/baloo_chettan"
            android:text="Profile Information"/>

        <TextView
            android:id="@+id/dis_lable"
            android:paddingLeft="20dp"
            android:layout_below="@id/pto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:paddingBottom="10dp"
            android:textSize="15dp"
            android:fontFamily="@font/amaranth_italic"
            android:text="This is the your Shop Profile information. Always update with correcy Details."/>


        <ScrollView
            android:layout_below="@+id/dis_lable"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/ic_shop_wall"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#1C2833"
                            android:text="Shop Name"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:layout_marginLeft="2dp"/>

                        <TextView
                            android:id="@+id/tv_mechanic_se_shop_name2"
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:fontFamily="@font/philosopher_regular"
                            android:textColor="#1C2833"
                            android:text="Shop Name"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/contact_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#1C2833"
                            android:text="Shop Contact No"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_contact"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:textColor="#1C2833"
                            android:fontFamily="@font/philosopher_regular"
                            android:hint="Shop Contact No"/>
                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="80dp"
                    android:fontFamily="@font/amaranth_italic"
                    android:textSize="14dp"
                    android:text="Please enter valid contact this is important"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/addres_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Shop Address"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_address"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:fontFamily="@font/philosopher_regular"
                            android:textColor="#1C2833"
                            android:hint="Location Address"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/city_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Location city"
                                    android:fontFamily="@font/baloobhai2_semibold"
                                    android:layout_marginLeft="2dp"/>

                                <EditText
                                    android:id="@+id/et_mechanic_se_shop_city"
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="18dp"
                                    android:paddingLeft="2dp"
                                    android:textColor="#1C2833"
                                    android:fontFamily="@font/philosopher_regular"
                                    android:hint="City"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="City Postalcode"
                                    android:fontFamily="@font/baloobhai2_semibold"
                                    android:layout_marginLeft="2dp"/>

                                <EditText
                                    android:id="@+id/et_mechanic_se_shop_postcode"
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="18dp"
                                    android:paddingLeft="2dp"
                                    android:textColor="#1C2833"
                                    android:fontFamily="@font/philosopher_regular"
                                    android:hint="Postal code"/>

                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/email_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Shop Email Address"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_email"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:textColor="#1C2833"
                            android:fontFamily="@font/philosopher_regular"
                            android:hint="Shop Email"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/website_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Shop Website"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_web"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:textColor="#1C2833"
                            android:fontFamily="@font/philosopher_regular"
                            android:hint="Shop Web Site"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="20dp"
                    android:layout_marginTop="50dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:text="Service Details"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/time_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Shop Open"
                                    android:fontFamily="@font/baloobhai2_semibold"
                                    android:layout_marginLeft="2dp"/>

                                <EditText
                                    android:id="@+id/et_mechanic_se_shop_open"
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="18dp"
                                    android:paddingLeft="2dp"
                                    android:fontFamily="@font/philosopher_regular"
                                    android:textColor="#1C2833"
                                    android:hint="Open Time"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Shop Close"
                                    android:fontFamily="@font/baloobhai2_semibold"
                                    android:layout_marginLeft="2dp"/>

                                <EditText
                                    android:id="@+id/et_mechanic_se_shop_close"
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="18dp"
                                    android:paddingLeft="2dp"
                                    android:textColor="#1C2833"
                                    android:fontFamily="@font/philosopher_regular"
                                    android:hint="Close Time"/>

                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="80dp"
                    android:textSize="14dp"
                    android:fontFamily="@font/amaranth_italic"
                    android:text="Please enter correct time after 'AM' or 'PM'"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/poya_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Poya Day State"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_poya"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:fontFamily="@font/philosopher_regular"
                            android:textSize="18dp"
                            android:textColor="#1C2833"
                            android:hint="Result"/>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="80dp"
                    android:textSize="14dp"
                    android:fontFamily="@font/amaranth_italic"
                    android:text="Please enter answer 'Yes' or 'No'"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/holiday_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:text="Other Holiday Note"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_holiday"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:fontFamily="@font/philosopher_regular"
                            android:textColor="#1C2833"
                            android:hint="Result"/>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="80dp"
                    android:textSize="14dp"
                    android:fontFamily="@font/amaranth_italic"
                    android:text="Please enter answer 'Yes' or 'No'"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/visit_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:text="Break Down Place Visit"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_visit"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:textColor="#1C2833"
                            android:fontFamily="@font/philosopher_regular"
                            android:hint="Result"/>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="80dp"
                    android:textSize="14dp"
                    android:fontFamily="@font/amaranth_italic"
                    android:text="Please enter answer 'Yes' or 'No'"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/ic_service_package"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Service Information"
                            android:fontFamily="@font/baloobhai2_semibold"
                            android:layout_marginLeft="2dp"/>

                        <EditText
                            android:id="@+id/et_mechanic_se_shop_service"
                            android:layout_gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:textSize="18dp"
                            android:textColor="#1C2833"
                            android:fontFamily="@font/philosopher_regular"
                            android:hint="Result"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="50dp"
                    android:layout_marginTop="30dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:fontFamily="@font/baloo_chettan"
                        android:text="Other Details"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:paddingLeft="20dp">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/rate_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Start Date"
                                    android:fontFamily="@font/baloobhai2_semibold"
                                    android:layout_marginLeft="2dp"/>

                                <TextView
                                    android:id="@+id/tv_mechanic_se_shop_start"
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="18dp"
                                    android:paddingLeft="2dp"
                                    android:textColor="#1C2833"
                                    android:fontFamily="@font/philosopher_regular"
                                    android:text="Start Date"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Register No"
                                    android:fontFamily="@font/baloobhai2_semibold"
                                    android:layout_marginLeft="2dp"/>

                                <TextView
                                    android:id="@+id/tv_mechanic_se_shop_regno"
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="18dp"
                                    android:paddingLeft="2dp"
                                    android:textColor="#1C2833"
                                    android:fontFamily="@font/philosopher_regular"
                                    android:text="Register No"/>

                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_mechanic_se_wrong"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:textSize="16dp"
                    android:layout_marginTop="15dp"/>


                <Button
                    android:id="@+id/btn_submit_se_shop_edite"
                    android:text="Edit Profile"
                    android:layout_marginTop="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginRight="70dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginBottom="40dp"
                    android:fontFamily="@font/headlandone_regular"
                    android:background="@drawable/button_shape" />


            </LinearLayout>

        </ScrollView>
    </RelativeLayout>

</LinearLayout>